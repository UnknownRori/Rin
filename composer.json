{
    "name": "unknownrori/rin",
    "description": "A lightweight framwork equivalent of expressjs",
    "type": "library",
    "require-dev": {
        "symfony/dependency-injection": "^6.1",
        "phpunit/phpunit": "^9.5",
        "predis/predis": "^2.0",
        "symfony/var-dumper": "^6.1"
    },
    "require": {
        "php": ">= 8.1",
        "psr/container": "^2.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "UnknownRori\\Rin\\": "src/"
        },
        "files": [
            "src/Bootstrap.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "UnknownRori\\Rin\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "UnknownRori",
            "email": "68576836+UnknownRori@users.noreply.github.com"
        }
    ],
    "archive": {
        "exclude": [
            "/tests/**/*"
        ]
    },
    "scripts": {
        "test": "phpunit"
    }
}